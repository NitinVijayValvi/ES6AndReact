/*8. Write a JavaScript program to check if a given string contains equal number of p's and t's 
present.*/

function equalPT(str){

	const strp = str.replace(/[^p]/g,"");
	
	const strt = str.replace(/[^t]/g,"");
	
	const num_p = strp.length;
	
	const num_t = strt.length;
	
	return num_p === num_t;
}

console.log(equalPT("passthatbookplease"));

console.log(equalPT("pushthatbutton"));
